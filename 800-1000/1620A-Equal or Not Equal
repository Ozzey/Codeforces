// https://codeforces.com/contest/1620/problem/A

#include <bits/stdc++.h>


using namespace std;
void fn(string s) {
    int n = s.size();

    int flag=0;

    for (int j=0;j<n;j++) {
        if (s[j]=='N')(flag++);
    }

    if(flag==1){cout<<"NO"<<endl;}
    else{cout<<"YES"<<endl;}


}


int main(){
    int t;
    cin >> t;
    while(t--) {
        string s;
        cin>>s;
        transform(s.begin(), s.end(), s.begin(), ::toupper);
        fn(s);
    }
    return 0;
}
